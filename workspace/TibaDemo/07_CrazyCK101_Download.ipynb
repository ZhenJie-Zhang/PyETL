    "import requests\n",
    "import json\n",
    "payload = {'key1': 'value1', 'key2': 'value2'}\n",
    "# 將查詢參數加入 POST 請求中\n",
    "html = requests.post(\"http://httpbin.org/post\", data=payload)\n",
    "print(html.text) #以json格式呈現\n",
    "json.loads(html.text)"
   ]
